<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1><%= title %></h1>
  <p>MOUNTAIN & COMEDY
    <%= title %>
  </p>


  <!-- CON EL USER LIST -->


  <ul>
    <%if(typeof(users)!=='undefined'){%>
      <% users.forEach((user)=>{%>
        <li>
          <p>
            <a href="/<%=user._id%>/show">
              <%=user.username%>
            </a>
            <%if(currentUser.role ==='Boss' && user.role!=='Boss'){%>
              <a href="/<%=user._id%>/delete">Delete</a>
              <%}%>
          </p>
        </li>
        <%})%>
          <%}%>
  </ul>

  <p class="account-message">
    <a href="/">Home</a>
  </p>


</body>

</html>
